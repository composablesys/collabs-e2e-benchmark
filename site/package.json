{
  "dependencies": {
    "@collabs/collabs": "0.5.1",
    "@collabs/ws-client": "0.5.1",
    "@collabs/ws-server": "0.5.1",
    "@material/mwc-button": "0.25.3",
    "@types/ws": "8.5.2",
    "automerge": "1.0.1-preview.7",
    "express": "4.17.1",
    "fabric": "2.7.0",
    "owebsync-js": "github:KristofJannes/owebsync-js",
    "reconnecting-websocket": "^4.4.0",
    "ws": "8.5.0",
    "y-websocket": "1.4.0",
    "yjs": "13.5.34"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/express": "4.17.13",
    "@types/fabric": "2.6.18",
    "@types/node": "15.6.1",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "html-webpack-plugin": "5.3.2",
    "npm-run-all": "4.1.5",
    "pkg-ok": "2.3.1",
    "prettier": "2.2.1",
    "rimraf": "2.7.1",
    "source-map-loader": "3.0.0",
    "ts-loader": "9.2.5",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "webpack": "5.50.0",
    "webpack-cli": "4.8.0"
  },
  "scripts": {
    "start": "npm run $FRAMEWORK",
    "yjs": "npm run static & npm run yjs-ws",
    "yjs-ws": "cross-env PORT=8081 y-websocket",
    "collabs": "npm run static & npm run collabs-ws",
    "collabs-ws": "cross-env PORT=8081 node build/server/collabs.js",
    "automerge": "npm run static & npm run automerge-ws",
    "automerge-ws": "cross-env PORT=8081 node build/server/collabs.js",
    "static": "node build/server/static.js",
    "dev": "npm-run-all dev:*",
    "dev:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack",
    "dev:server": "npm run build:server",
    "build": "npm-run-all build:*",
    "build:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack --mode production --devtool source-map",
    "build:server": "tsc --build tsconfig-server.json",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf build"
  },
  "engines": {
    "node": ">=16"
  }
}
